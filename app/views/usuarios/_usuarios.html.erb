<h2>Embajadores</h2>
<div id="promotores">
<ul>
	<% for user in current_usuario.promotores %>
	<li>
		<%= user.nombre_usuario %>
	</li>
<% end %>	
</ul>
</div>
<br><br>
<% if can? :manage, Sorteo %>
<h2>Administradores</h2>
<div id="administradores">
	<ul>
		<% for user in Usuario.with_role(:admin) %>
		<li>
			<%= user.nombre_usuario %>
		</li>
	<% end %>	
	</ul>
</div>
<% end %>